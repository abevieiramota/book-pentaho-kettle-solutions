           <schema_name/>
           </partitioning>
    <connection>sakila</connection>
    <sql>SELECT
	  f.film_id
	, IFNULL(f.title, &apos;&apos;) AS title
	, IFNULL(f.description, &apos;&apos;) AS description
	, IFNULL(f.release_year, 0) AS release_year
	, IFNULL(f.rental_duration, 0) AS rental_duration
	, IFNULL(f.rental_rate, 0) AS rental_rate
	, IFNULL(f.length, 0) AS length
	, IFNULL(f.replacement_cost, 0) AS replacement_cost
	, IFNULL(f.rating, &apos;&apos;) AS rating
	, IFNULL(f.special_features, &apos;&apos;) AS special_features
	, IFNULL(l.name, &apos;&apos;) AS language
	, IFNULL(ol.name, &apos;&apos;) AS original_language
	, IFNULL(f.last_update, &apos;&apos;) AS last_update
	, &apos;sakila-db.film&apos; AS record_source
	, NOW() AS load_end_dts
FROM
	film f
LEFT OUTER JOIN
	language l
USING
	(language_id)
LEFT OUTER JOIN
	language ol
ON
	(f.original_language_id = ol.language_id)
ORDER BY 1
</sql>
