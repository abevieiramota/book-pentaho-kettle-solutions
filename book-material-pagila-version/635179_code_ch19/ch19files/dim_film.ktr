    <name>input data vault</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>sakila_data_vault</connection>
    <sql>SELECT
  sf.last_update AS film_last_update
, hf.film_id AS film_id
, hf.hub_film_id
, sf.title AS film_title
, sf.description AS film_description
, sf.release_year AS film_release_year
, sf.language AS film_language
, IFNULL(sf.original_language, &apos;Not Applicable&apos;) AS film_original_language
, sf.rental_duration AS film_rental_duration
, sf.rental_rate AS film_rental_rate
, sf.length AS film_duration
, sf.replacement_cost AS film_replacement_cost
, sf.rating AS film_rating_code
, sf.special_features AS tmp_special_features
FROM
	sat_film sf
INNER JOIN 
	hub_film hf
USING
	(hub_film_id)
WHERE
	sf.load_end_dts IS NULL
ORDER BY
	hf.film_id</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
