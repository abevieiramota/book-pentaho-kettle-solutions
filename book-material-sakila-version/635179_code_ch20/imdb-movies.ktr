    <script><![CDATA[^
(                            #1
(                            #2
  ("([^"]+)")                #3,4: series title
| ([^"\t][^\t]*)             #5    movie title
)                            #/2
\x20
\(
  (                          #6
    (\d{4})                  #7: year
  | (\?{4})                  #8: year unknown (or unspecified)
  )                          #/6
  (/([IVX]+))?               #9,10: roman numeral (denotes part?)
\)
(\x20\{([^\}]+)\})?          #11,12: subtitle (usually applicable to series)
(\x20\((V|VG|TV|mini)\))?    #13,14: type (video, video game, tv series or mini series. Otherwise cinematic movie or regular series - see groups 2,3,4)
(\x20\{\{SUSPENDED\}\})?     #15: suspended?
)
\t+
(                            #15
  (                          #16
    ((\d{4})|(\?{4}))        #17,18: year, 19: unknown year
    (                        #20
      \x20?-\x20?
      ((\d{4})|(\?{4}))      #21, 22: year; 23: unknown year
    )?                       #/20
  )                          #/16
  (,                         #24
    ((\d{4})|(\?{4}))        #25, 26: year; 27 unknown year;
    (                        #28
      \x20?-\x20?
      ((\d{4})|(\?{4}))      #29, 30: year; 31: unknown year
    )?                       #/28
  )*                         #/24
)?                           #/15
(\t+                         #32
  \(                         
     (\d)+                   #33: number of episodes or parts
     \x20
     ([Ee]pisodes|parts)     #34 episodes or parts
  \)
)?                           #/32
(\t+                                          #35
  \(
     (                                        #36 status
       (partially\x20)?shot(\x20(in|since))?  #37: partially; 38; 39: in/since
     |  fragment\x20first\x20released         
     |  first\x20shown
     |  completed
     )                                        #/36
     \x20
     (                                        #40
       ([\?]{4})                              #41: unknown year
     | ((~)?(\d{4}))                          #42, 43: approximate?; 44: year
     )                                        #/40
     (                                        #45
       (/(\d{2}))                             #46, 47
     | (\x20?-\x20?(\d{4}))                   #48, 49: year
     )?                                       #/42
  \)
)?                                            #/35
(                                             #50
  [\t\x20]+
  \(
    (unfinished(\x20and\x20unreleased)?|unreleased|[E|e]xtended\x20[C|c]ut|working\x20title) #51: unfinished/unreleased, 52
  \)
)?                                            #/49
$]]></script>    <matcher>line_text</matcher>
