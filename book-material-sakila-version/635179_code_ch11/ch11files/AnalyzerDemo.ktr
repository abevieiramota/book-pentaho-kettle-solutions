    <sql>SELECT
     dim_customer.`customer_country` AS customer_country,
     dim_customer.`customer_city` AS customer_city,
     dim_date.`year4` AS year4,
     dim_date.`month_number` AS month_number,
     dim_date.`year_month_number` AS year_month_number,
    dim_film.`film_title` AS film_title,
     dim_film.`film_release_year` AS film_release_year,
     dim_film.`film_in_category_action` AS film_in_category_action,
     dim_film.`film_in_category_animation` AS film_in_category_animation,
     dim_film.`film_in_category_comedy` AS film_in_category_comedy,
     dim_film.`film_in_category_drama` AS film_in_category_drama,
     dim_film.`film_in_category_family` AS film_in_category_family,
     sum(fact_rental.`count_rentals`) rentals
     , sum(amounts.amount) rental_amount
FROM
     `dim_film` dim_film INNER JOIN `fact_rental` fact_rental ON dim_film.`film_key` = fact_rental.`film_key`
     INNER JOIN `dim_customer` dim_customer ON fact_rental.`customer_key` = dim_customer.`customer_key`
     INNER JOIN `dim_date` dim_date ON fact_rental.`date_key` = dim_date.`date_key`
     INNER JOIN amounts ON amounts.rental_id = fact_rental.rental_id
GROUP BY
     customer_country,
     customer_city,
     year4,
     month_number,
     year_month_number,
     film_title,
     film_release_year,
     film_in_category_action,
     film_in_category_animation,
     film_in_category_comedy,
     film_in_category_drama,
     film_in_category_family</sql>
